    9  curl -fsSL https://get.docker.com -o get-docker.sh
   10  sudo sh get-docker.sh
   11  sudo usermod -aG docker ${USER}
   12  docker run hello-world
   13  sudo apt-get install -y python3 python3-pip libffi-dev
   14  sudo pip3 install docker-compose
   16  curl -Lo minikube https://storage.googleapis.com/minikube/releases/latest/minikube-linux-arm64   && chmod +x minikube
   17  sudo mkdir -p /usr/local/bin/
   18  sudo install minikube /usr/local/bin/
   19  minikube start --driver=kvm2
   20  minikube get pods
   21  curl -LO "https://dl.k8s.io/release/$(curl -L -s https://dl.k8s.io/release/stable.txt)/bin/linux/arm64/kubectl"
   22  chmod +x kubectl
   23  sudo mv ./kubectl /usr/local/bin/kubectl
   24  kubectl version --client
   25  kubectl get pods -A
   26  minikube start --driver=kvm2
   27  minikube start
